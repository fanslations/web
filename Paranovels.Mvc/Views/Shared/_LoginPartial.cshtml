@using Microsoft.AspNet.Identity

<ul class="nav navbar-nav navbar-right">
    @if (Request.IsAuthenticated)
    {
        <li>
            <a href="@Url.Action("Manage","Account")"><i class="fa fa-user"></i>@User.Identity.GetUserName()</a>
        </li>
        
    }
    else
    {

        <li><a href="/account/login/" id="loginLink"><i class="fa fa-user"></i>LOG IN | SIGN UP</a></li>

    }
</ul>